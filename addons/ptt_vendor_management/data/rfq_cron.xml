<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!--
        RFQ Cron Job
        
        Automatically closes RFQs past their closing date and selects
        the lowest-priced vendor.
    -->
    <!-- DISABLED - Auto-close RFQs past due -->
    <record id="ir_cron_auto_close_rfqs" model="ir.cron">
        <field name="name">Vendor RFQ: Auto-Close Past Due</field>
        <field name="model_id" ref="model_ptt_vendor_rfq"/>
        <field name="state">code</field>
        <field name="code">model.cron_auto_close_rfqs()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="False"/>
    </record>
    
</odoo>

        <!-- CARICATURE ARTIST - PRODUCT TEMPLATE (9 VARIANTS) -->
        <!-- Creates variants: Event Type × Service Tier  -->
        <!-- No variant-specific pricing - base price only -->
        <!-- ============================================ -->
        <record id="product_template_caricature_artist" model="product.template">
            <field name="name">Caricature Artist [UPDATED]</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="True"/>
            <field name="description_sale">Caricature artist services. Select Event Type and Service Tier for your event.</field>
        </record>
        
        <record id="caricature_event_type_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="product_template_caricature_artist"/>
            <field name="attribute_id" ref="pa_event_type"/>
            <field name="value_ids" eval="[(6, 0, [ref('pav_event_social'), ref('pav_event_corporate'), ref('pav_event_wedding')])]"/>
        </record>
        <record id="caricature_service_tier_attribute_line" model="product.template.attribute.line">

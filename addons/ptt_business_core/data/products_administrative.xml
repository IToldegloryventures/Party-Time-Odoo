<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- 
        Administrative and Billing Products
        Fees, Deposits, Discounts, Insurance, etc.
    -->
    <data noupdate="0">
        <!-- ============================================ -->
        <!-- ADDITIONAL INSURED COVERAGE EXTENSION       -->
        <!-- ============================================ -->
        <record id="product_template_additional_insured" model="product.template">
            <field name="name">* Additional Insured Coverage Extension</field>
            <field name="default_code">INS-EXT</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Additional insured coverage extension for event liability.</field>
        </record>

        <!-- ============================================ -->
        <!-- CLIENT CONTRACT - BS                        -->
        <!-- ============================================ -->
        <record id="product_template_client_contract" model="product.template">
            <field name="name">* Client Contract - BS</field>
            <field name="default_code">CONTRACT</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">consu</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Client contract (non-inventory single).</field>
        </record>

        <!-- ============================================ -->
        <!-- CLIENT DISCOUNTS                            -->
        <!-- ============================================ -->
        <record id="product_template_client_discount" model="product.template">
            <field name="name">* Client Discounts</field>
            <field name="default_code">DISCOUNT</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Client discount applied to order.</field>
        </record>

        <!-- ============================================ -->
        <!-- CLIENT REFUND                               -->
        <!-- ============================================ -->
        <record id="product_template_client_refund" model="product.template">
            <field name="name">* Client Refund</field>
            <field name="default_code">REFUND</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Client refund applied to order.</field>
        </record>

        <!-- ============================================ -->
        <!-- CUSTOMER DEPOSIT / RETAINER                 -->
        <!-- ============================================ -->
        <record id="product_template_customer_deposit" model="product.template">
            <field name="name">* Customer Deposit/Retainer</field>
            <field name="default_code">DEPOSIT</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">consu</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Customer deposit or retainer payment (non-inventory single).</field>
        </record>

        <!-- ============================================ -->
        <!-- CANCELLATION EVENT FEES                     -->
        <!-- ============================================ -->
        <record id="product_template_cancellation_fees" model="product.template">
            <field name="name">* Cancellation Event Fees</field>
            <field name="default_code">CANCEL-FEE</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Cancellation fees for event cancellation.</field>
        </record>

        <!-- ============================================ -->
        <!-- INITIAL 10% LATE FEE (Non-Taxable)          -->
        <!-- ============================================ -->
        <record id="product_template_late_fee_10" model="product.template">
            <field name="name">* Initial 10% Late Fee</field>
            <field name="default_code">LATE-10</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="taxes_id" eval="[(5, 0, 0)]"/>
            <field name="description_sale">Initial 10% late fee (non-taxable).</field>
        </record>

        <!-- ============================================ -->
        <!-- MONTHLY 1.5% LATE FEE (Non-Taxable)         -->
        <!-- ============================================ -->
        <record id="product_template_late_fee_monthly" model="product.template">
            <field name="name">* Monthly 1.5% Late Fee</field>
            <field name="default_code">LATE-1.5</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="taxes_id" eval="[(5, 0, 0)]"/>
            <field name="description_sale">Monthly 1.5% late fee (non-taxable).</field>
        </record>

        <!-- ============================================ -->
        <!-- VARIABLE CONSIDERATION                      -->
        <!-- ============================================ -->
        <record id="product_template_variable_consideration" model="product.template">
            <field name="name">* Variable Consideration (Performance Bonus &amp; Penalties)</field>
            <field name="default_code">VAR-CONSID</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="description_sale">Variable consideration for performance bonuses and penalties.</field>
        </record>

        <!-- ============================================ -->
        <!-- MISC EVENT ENTERTAINMENT SERVICES (Untaxed) -->
        <!-- ============================================ -->
        <record id="product_template_misc_entertainment_untaxed" model="product.template">
            <field name="name">* Misc Event Entertainment Services (Untaxed)</field>
            <field name="default_code">MISC-ENT-UT</field>
            <field name="categ_id" ref="product_category_event_entertainment"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="True"/>
            <field name="taxes_id" eval="[(5, 0, 0)]"/>
            <field name="description_sale">Miscellaneous event entertainment services (non-taxable).</field>
        </record>

        <!-- ============================================ -->
        <!-- MISC EVENT SERVICES (Taxed)                 -->
        <!-- ============================================ -->
        <record id="product_template_misc_services_taxed" model="product.template">
            <field name="name">* Misc Event Services (Taxed)</field>
            <field name="default_code">MISC-SVC</field>
            <field name="categ_id" ref="product_category_event_services"/>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="True"/>
            <field name="description_sale">Miscellaneous event services (taxable).</field>
        </record>
    </data>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--
        Entertainment Products & Services
        ALL services have: Event Type (Social, Corporate, Wedding) × Service Tier (Essential, Classic, Premier)
        Services with additional variants also have their type-specific attributes
        
        Based on: QuickBooks ProductsServicesList export
    -->
    <data noupdate="1">
        
        <!-- ================================================================ -->
        <!-- DJ SERVICES - Event Type × Service Tier (9 variants)            -->
        <!-- ================================================================ -->
        <record id="product_template_dj_services" model="product.template">
            <field name="name">DJ Services</field>
            <field name="default_code">DJ-SVC</field>
            <field name="categ_id" ref="product_category_event_entertainment"/>
            <field name="type">service</field>
            <field name="list_price">300.0</field>
            <field name="standard_price">150.0</field>
            <field name="uom_id" ref="uom.product_uom_hour"/>
            
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="True"/>
            <field name="service_tracking">task_in_project</field>
            <field name="description_sale">Professional DJ services. Pricing varies by Event Type and Service Tier.</field>
        </record>
        <record id="dj_event_type_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="product_template_dj_services"/>
            <field name="attribute_id" ref="pa_event_type"/>
            <field name="value_ids" eval="[(6, 0, [ref('pav_event_social'), ref('pav_event_corporate'), ref('pav_event_wedding')])]"/>
        </record>
        <record id="dj_service_tier_attribute_line" model="product.template.attribute.line">
            <field name="product_tmpl_id" ref="product_template_dj_services"/>
            <field name="attribute_id" ref="pa_service_tier"/>
            <field name="value_ids" eval="[(6, 0, [ref('pav_tier_essentials'), ref('pav_tier_classic'), ref('pav_tier_premier')])]"/>
        </record>


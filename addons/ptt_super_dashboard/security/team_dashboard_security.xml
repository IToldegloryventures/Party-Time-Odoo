<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Team Dashboard Security Groups -->
    <record id="group_team_dashboard_manager" model="res.groups">
        <field name="name">Team Dashboard Manager</field>
        <field name="category_id" ref="base.module_category_tools"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_team_dashboard_viewer" model="res.groups">
        <field name="name">Team Dashboard Viewer</field>
        <field name="category_id" ref="base.module_category_tools"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
